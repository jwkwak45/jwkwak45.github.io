        // 각 객체의 name,duedate,created_date,done,myday 를 기준으로 내림과 올림 차순으로 배열함.


        {"id":2, //자신이 저장된 리스트 아이디를 공유. 이 아이디 값을 통해 같은 아이디를 가진 값을 불러옴..
        "name":"할일 1",
        "myday":false, //false이면 기본값, true면 나의 하루에 추가된것으로 가정
        "duedate":null,//기본값은 없음. 추가되면 날짜값으로 저장, 없을 때는 표시 X
        "fre_alert":null, // 알림 설정도 기본값은 X
        "note": null, //역시 기본값 x
        "created_date": "2017:11:11:11:58", //현재 시간이 분 단위 까지 저장되게... 그러나 직접 들어가지 않으면 표시되지 않으므로 생략.
                                        //(이를 통해 나중에 만든지 얼마나 지났는지 분단위까지 정확하게 알려 줄 수 있음)
        "repeat":null, //역시 기본값 x
        "done":false //기본값 false

        },
        { "id":2, 
            "name":"할일 2",
            "myday":true, 
            "duedate":"2017:11:15", // 있으면(not null 일때) 기한 아이콘과 함께 날짜까지 표시됨. 오늘이나 내일일 때는 "오늘까지", "내일까지" 로 표시
            "fre_alert":"2017:11:15:11:50", // 알람도 분 단위까지 저장됨. 있으면 알람 아이콘으로 표시.
            "note": "hi", // 있으면 노트 아이콘으로 표시
            "repeat":[{"weeks":"3"},
                 {"weekday":["mon", "tue"]},
                 {"month":"11"},
                 {"date":"11"}], 
                 // 있으면 반복 아이콘으로 표시.
                 //반복되는 주와, 반복되는 요일, 1달이나 1년 기준일시 일 정보(1달 반복), 월,일 정보(1년기준)가 필요
            "created_date": "2017:11:11:11:58", //현재 시간이 분 단위 까지 저장되게... 그러나 직접 들어가지 않으면 표시되지 않으므로 생략.
                 //(이를 통해 나중에 만든지 얼마나 지났는지 분단위까지 정확하게 알려 줄 수 있음)
            "done":true // true 이면 가운데 줄이 생기고, 체크가 되어있는 아이콘으로 바뀜 (PC 버전은 체크되었을때 소리도 남.)
         }


    ]


